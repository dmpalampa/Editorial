<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.monitor.editorial.entities.Entity_Users"
		table="users" schema="public" catalog="postgres">

		<id column="user_id" name="userId">
			<generator class="identity"></generator>
		</id>
		<property name="firstName">
			<column name="first_name"/>
		</property>
		<property name="lastName">
			<column name="last_name"/>
		</property>
		<property name="username">
			<column name="username"/>
		</property>
		<property name="password">
			<column name="password" />
		</property>
		<property name="createdOn">
			<column name="created_on"/>
		</property>
		<property name="createdBy">
			<column name="created_by" />
		</property>
		<property name="updatedOn">
			<column name="updated_on"/>
		</property>
		<property name="updatedBy">
			<column name="updated_by" />
		</property>
		<property name="roleId">
			<column name="role_id" />
		</property>
		<property name="role">
			<column name="role" />
		</property>
		<property name="status">
			<column name="status" />
		</property>
		<property name="idCardNumber">
			<column name="id_card_number" />
		</property>
		<property name="telPhoneNumber1">
			<column name="tel_phone_number1"/>
		</property>

		<property name="telPhoneNumber2">
			<column name="tel_phone_number2"/>
		</property>

	<property name="editorOfCategory">
			<column name="editor_of_category"/>
		</property>

	</class>
</hibernate-mapping>