<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.monitor.editorial.entities.Entity_StoryCategory" table="story_category" schema="public"
           catalog="postgres">
         <id column="s_category_id" name="sCategoryId">
            <generator class="identity"></generator>
        </id>
        <property name="categoryNumber">
            <column name="category_number"/>
        </property>
        <property name="categoryName">
            <column name="category_name"/>
        </property>
        <property name="createdOn">
            <column name="created_on"  />
        </property>
        <property name="updatedOn">
            <column name="updated_on"/>
        </property>
        <property name="createdBy">
            <column name="created_by" />
        </property>
        <property name="updateBy">
            <column name="update_by"  />
        </property>
        <property name="version">
            <column name="version" />
        </property>
           <property name="supervisor">
            <column name="supervisor" />
        </property>
        
              <property name="description">
            <column name="description" />
        </property>
    </class>
</hibernate-mapping>